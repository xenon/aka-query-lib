{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES6",
        "declaration": true,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "outDir": "./dist",
        "strict": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "moduleResolution": "node"
    },
    "include": ["lib"],
    "exclude": ["example"]
}
